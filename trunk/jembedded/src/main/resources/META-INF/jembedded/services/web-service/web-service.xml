<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="webServiceEngine" name="webServiceEngine" class="org.jsemantic.jembedded.services.webservice.engine.impl.WebServiceEngineImpl" autowire="byName" lazy-init="true" scope="prototype"/>
		
	<bean id="webHttpService" name="hhtpService" class="org.jsemantic.jembedded.services.httpservice.impl.HttpServiceImpl" autowire="byName" lazy-init="true" scope="prototype">	
		<property name="port" value="9006"/>
		<property name="host" value="localhost"/>
		<property name="rootContext" value="/test"/>
		<property name="webApplication" value="src/main/resources/webapp/webservice"/>
	</bean>
	
	<bean id="webServiceHttpClient" class="org.jsemantic.jembedded.services.httpservice.client.HttpTestClientImpl" lazy-init="true" scope="prototype"/>
	
</beans>
